{
  "name": "chronoframe",
  "version": "1.0.0-beta.2",
  "author": {
    "name": "Timothy Yin",
    "email": "master@uniiem.com"
  },
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "db": "drizzle-kit",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "build": "nuxt build",
    "build:deps": "pnpm --filter @chronoframe/webgl-image build",
    "dev": "run-p dev:dep dev:wait-only",
    "dev:dep": "pnpm --filter @chronoframe/webgl-image dev",
    "dev:only": "nuxt dev",
    "dev:wait-only": "wait-on packages/webgl-image/dist && nuxt dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@chronoframe/webgl-image": "workspace:*",
    "@indoorequal/vue-maplibre-gl": "^8.4.2",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxthub/core": "0.9.0",
    "@nuxtjs/i18n": "10.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/vite": "^4.1.15",
    "@unhead/vue": "^2.0.5",
    "@vingle/bmp-js": "^0.2.5",
    "@vueuse/nuxt": "13.9.0",
    "@yeger/vue-masonry-wall": "^5.1.0",
    "better-sqlite3": "^12.2.0",
    "dayjs-nuxt": "2.1.11",
    "drizzle-orm": "^0.44.4",
    "es-toolkit": "^1.40.0",
    "exiftool-vendored": "^30.3.0",
    "file-type": "^21.0.0",
    "heic-convert": "^2.1.0",
    "heic-to": "^1.2.2",
    "maplibre-gl": "^5.9.0",
    "motion-v": "^1.7.4",
    "nuxt": "^4.1.2",
    "nuxt-auth-utils": "0.5.23",
    "nuxt-gtag": "4.0.0",
    "nuxt-mapbox": "1.6.4",
    "nuxt-maplibre": "^1.1.1",
    "nuxt-og-image": "5.1.12",
    "pinia": "^3.0.3",
    "reka-ui": "^2.5.1",
    "sharp": "^0.34.4",
    "swiper": "^12.0.3",
    "systeminformation": "^5.27.11",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "thumbhash": "^0.1.1",
    "tippy.js": "^6.3.7",
    "unstorage": "^1.15.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "wait-on": "^9.0.1",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.18.3",
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/fluent-emoji-flat": "^1.2.5",
    "@iconify-json/lucide": "^1.2.70",
    "@iconify-json/material-symbols": "^1.2.33",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.59",
    "@iconify-json/streamline": "^1.2.5",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@iconify-json/tabler": "^1.2.22",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/heic-convert": "^2.1.0",
    "consola": "^3.4.2",
    "dayjs": "^1.11.15",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "mapbox-gl": "^3.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.0.0",
    "vitepress": "2.0.0-alpha.12"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@nuxt/kit"
      ]
    },
    "overrides": {
      "sharp": "0.34.4",
      "@vue/devtools-kit": "7.7.9"
    },
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ],
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp"
    ]
  }
}